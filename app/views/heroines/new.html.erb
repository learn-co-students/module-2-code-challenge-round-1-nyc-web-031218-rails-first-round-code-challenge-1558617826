  <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
  <% end %>
  
<h1>New Heroine Form</h1>

<%= form_for @heroine do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :super_name %>
<%= f.text_field :super_name %><br>
<%= f.collection_select :power_id, @powers, :id, :name %><br>
<%= f.submit %>

<% end %><br>

<%= button_to "Index", heroines_path, method: :get %>
